import numpy as np
numbers = np.array(range(1, 11), copy = True)
numbers

ones = np.ones([2, 4], dtype = np.float64)
ones

zeros = np.zeros([2, 4], dtypr = np.float64)
zeros

empty = np.empty([2, 4], dtype = np.float64)
empty

eye = np.eye(3)
eye

sap = np.array(["MMM", "ABT", "ABBV", "ACN", "ACE", "ATVI", "ADBE", "ADT"])
sap
